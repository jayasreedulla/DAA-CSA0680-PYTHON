def word_break(s, wordDict):
    n = len(s)
    dp = [False] * (n + 1)
    dp[0] = True 
    for i in range(1, n + 1):
        for word in wordDict:
            if i >= len(word) and dp[i - len(word)] and s[i - len(word):i] == word:
                dp[i] = True
                break
    return dp[n]
s = "leetcode"
wordDict = ["leet", "code"]
print("Input:")
print("s =", s)
print("wordDict =", wordDict)
print("Output:", word_break(s, wordDict))
